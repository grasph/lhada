function btag_eff
       #Returns the b-tagging efficiency of a b jet
       arg bjet #b jet to apply efficiency to
       code CMS-PAS-SUS-015-functions.cc
	 
function mt2
       #Computes the M_{T2} observable, http://arXiv.org/abs/hep-ph/9906349v1
       arg particle1 #quadrivector of first visible particle (only m, px, py are used)
       arg particle2 #quadrivector of first visible particle (only m, px, py are used)
       arg met       #quadrivector of missing energy (only px, py are used)       
       code CMS-PAS-SUS-015-functions.cc

#cut trigger

cut mt2_cut
    select (jets.size < 2) or (mt2(particle1 = jets[1], particle2 = jets[2], met = met) > 200)
    
cut deltaphi_etmiss_jet
    select (dphi(met, jets[1]) > 0.3) and (dphi(met, jets[2]) > 0.3) and(dphi(met, jets[3]) > 0.3) and(dphi(met, jets[4]) > 0.3)
    
#cut etmiss_non_had_frac
#cut veto
#    select (lep_for_veto.size < 1) and (obj_for_tau_veto.size < 1)

cut preselection
#    select trigger
    select mt2_cut
    select deltaphi_etmiss_jet
#    select etmiss_non_had_frac
    select veto

cut one_jet_selection
    select preselection
    select jets.size = 1
    select jets[1].pt > 200

cut at_least_two_jet_selection
    select preselection
    select jets.size() > 1
    select mt2_cut > 200
    
   
# Regions
cut 1j_loose_1
    select one_jet_selection
    select ht > 575

cut 1j_loose_2
    select at_least_two_jet_selection
    select bjets.size < 3
    select ht > 575
    select ht < 1000
    select mt2_cut > 200

cut 1j_medium_1
    select one_jet_selection
    select bjets.size < 1
    select ht > 1000

cut 1j_medium_2
    select one_jet_selection
    select bjets.size > 0
    select ht > 575

cut 1j_medium_3
    select at_least_two_jet_selection
    select jets.size < 4
    select bjets.size < 1
    select ht > 575
    select ht < 1000
    select mt2_cut > 800

cut 1j_medium_4
    select at_last_two_jet_selection
    select jets.size < 4
    select bjets.size > 0
    select bjets.size < 3
    select ht > 575
    select ht < 1000
    select mt2_cut > 600

cut 1j_medium_5
    select at_last_two_jet_selection
    select jets.size < 4
    select bjets.size < 2
    select ht > 1000
    select ht < 1500
    select mt2_cut > 800

cut 1j_medium_6
    select at_last_two_jet_selection
    select jets.size < 4
    select bjets.size = 2
    select ht > 1000
    select ht < 1500
    select mt2_cut > 400

cut 1j_medium_7
    select at_last_two_jet_selection
    select jets.size < 4
    select bjets.size < 2
    select ht > 1500
    select mt2_cut > 400

cut 1j_medium_8
    select at_last_two_jet_selection
    select jets.size < 4
    select bjets.size = 2
    select ht > 1500
    select mt2_cut > 200


