object jets_eta47
       take external
       apply antikt(dR=0.4, ptmin=30, etamax=4.7)

object jets
       take jets_eta47
       select eta < 2.4.

object bjets
       take external
       apply antikt_b(dR=0.4, ptmin=30, etamax=2.5)
       weight btag_eff

object met
       take external


object ht
       take jets
       apply scalar_pt_sum

